<h1>Videos (<%= @videos.length %>)</h1>
<%= link_to "New Video", "/videos/new" %>

<% @videos.each do |video| %>
<div>
	<p><%= link_to "#{video.title}, #{video.artist}", video_path(video)  %></p>
</div>
<iframe width="560" height="315" src="<%= convert_url(video.url) %>" frameborder="0" allowfullscreen></iframe> 
<% end %>