<h1>Add a Video</h1>
<%= form_tag "/videos" do %>
  <p>
    <%= label_tag "artist" %><br>
    <%= text_field_tag "artist" %>
  </p>
  <p>
    <%= label_tag "title" %><br>
    <%= text_field_tag "title" %>
  </p>
  <p>
    <%= label_tag "url" %><br>
    <%= text_field_tag "url" %>
  </p>
  <p>
    <%= submit_tag "Save" %>
  </p>
<% end %>